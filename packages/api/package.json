{
  "name": "@athlon1600/chat-sdk-js",
  "version": "0.0.12",
  "description": "SDK to communicate with Chat backend via HTTP and Websocket protocols",
  "main": "./dist-cjs/index.js",
  "module": "./dist-es/index.js",
  "types": "./dist-types/index.d.ts",
  "browser": "./dist/bundle.js",
  "files": [
    "dist-*",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/athlon1600/chat",
    "directory": "packages/api"
  },
  "scripts": {
    "build": "concurrently \"npm run build:cjs\" \"npm run build:es\" \"npm run build:types\" ",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:es": "tsc -p tsconfig.es.json",
    "build:types": "tsc -p tsconfig.types.json",
    "build:bundle": "webpack --mode development",
    "build:docs": "typedoc --tsconfig tsconfig.cjs.json src/index.ts",
    "clean": "rimraf ./dist* && rimraf ./node_modules",
    "prepare": "npm run clean && npm run build"
  },
  "keywords": [],
  "author": "Athlon1600",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.2.1",
    "socket.io-client": "^4.5.4",
    "webpack": "^5.75.0"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "rimraf": "^3.0.2",
    "typedoc": "^0.23.23"
  }
}